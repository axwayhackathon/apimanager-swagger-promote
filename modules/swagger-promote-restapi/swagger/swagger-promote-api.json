{
    "swagger": "2.0",
    "info": {
        "title": "Swagger Promote API",
        "version": "1.0.0"
    },
    "host": "example.com",
    "paths": {
        "/api": {
            "get": {
                "responses": {
                    "200": {
                        "description": "",
                        "schema": {
                            "type": "object",
                            "properties": {}
                        }
                    }
                },
                "summary": "Get the actual API",
                "operationId": "export"
            },
            "post": {
                "responses": {
                    "200": {
                        "description": "",
                        "schema": {
                            "type": "object",
                            "properties": {}
                        }
                    }
                },
                "summary": "Replicate the desired API state",
                "operationId": "import",
                "parameters": [
                    {
                        "in": "formData",
                        "name": "configFile",
